{
  "name": "focusflow-tauri",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "tauri dev",
    "dev:web": "vite",
    "dev:reset": "powershell -ExecutionPolicy Bypass -File scripts/dev-reset.ps1",
    "dev:resetdb": "powershell -ExecutionPolicy Bypass -File scripts/dev-reset.ps1 -ResetDb",
    "build:web": "tsc -b && vite build",
    "build": "tauri build",
    "preview": "vite preview",
    "typecheck": "tsc -b --pretty false",
    "lint": "eslint .",
    "format": "prettier -w .",
    "check": "npm run typecheck && npm run lint",
    "tauri": "tauri"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.89.0",
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-shell": "^2.0.0",
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^8.45.0",
    "framer-motion": "^10.0.0",
    "globals": "^13.0.0",
    "lucide-react": "^0.263.0",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.0.0",
    "vite": "^4.0.0"
  }
}
